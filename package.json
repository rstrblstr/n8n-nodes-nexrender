{
  "name": "n8n-nodes-nexrender",
  "version": "0.0.13",
  "description": "nexrender automation",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "http://ccroww.com",
  "author": {
    "name": "ccroww",
    "email": "contactme@my.website.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rstrblstr/n8n-nodes-nexrender.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NexrenderApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Nexrender/Nexrender.node.js",
      "dist/nodes/Nexrender/Nexapi/index.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@nexrender/types": "^1.45.6",
    "node-fetch": "^2.6.12"
  }
}
